{
  "private": "true",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vitepress build --base /docs/",
    "build-mpa": "vitepress build --mpa --base /docs/",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "dev": "vitepress --base /docs/",
    "postinstall": "husky install",
    "lint": "prettier  --write . && eslint --fix --ext .js,.ts,.vue",
    "lint:check": "pnpm lint:prettier && pnpm lint:eslint && pnpm lint:zh",
    "lint:eslint": "eslint . --ext .js,.ts,.vue",
    "lint:md": "markdownlint **/*.md && lint:zh",
    "lint:prettier": "prettier --check .",
    "lint:zh": "zhlint 'docs/**/*.md'",
    "serve": "vitepress serve --base /docs/",
    "site:preview": "vitepress build --outDir _dist"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "prettier": "prettier-config-vuepress",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@iconify-json/ic": "^1.1.14",
    "@iconify-json/logos": "^1.1.34",
    "@iconify-json/mdi": "^1.1.54",
    "@iconify-json/ph": "^1.1.6",
    "@mdit/plugin-container": "^0.4.8",
    "@mdit/plugin-mark": "^0.4.8",
    "@types/js-yaml": "^4.0.5",
    "@vueuse/core": "^10.3.0",
    "@vueuse/integrations": "^10.3.0",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.9",
    "feed": "^4.2.2",
    "js-yaml": "^4.1.0",
    "markdown-it-footnote": "^3.0.3",
    "medium-zoom": "^1.0.8",
    "qrcode": "^1.5.3",
    "sass": "^1.64.2",
    "typescript": "^5.1.6",
    "unocss": "^0.54.2",
    "vitepress": "^1.0.0-rc.20",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "vue-router-better-scroller": "^0.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@commitlint/types": "^17.4.4",
    "@mdit/plugin-figure": "^0.4.8",
    "@mdit/plugin-img-lazyload": "^0.4.8",
    "@mdit/plugin-img-mark": "^0.4.8",
    "@mdit/plugin-img-size": "^0.4.8",
    "@mdit/plugin-sub": "^0.4.8",
    "@mdit/plugin-sup": "^0.4.8",
    "@types/eslint": "^8.44.2",
    "@types/markdown-it": "^13.0.0",
    "@types/node": "^18.17.4",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@unocss/eslint-config": "^0.54.2",
    "@vite-pwa/vitepress": "^0.2.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "conventional-changelog-cli": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.46.0",
    "eslint-config-vuepress": "^4.3.0",
    "eslint-config-vuepress-typescript": "^4.4.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-vue": "9.16.1",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "markdown-it": "^13.0.1",
    "markdownlint-cli": "^0.35.0",
    "pnpm": "^8.6.12",
    "prettier": "^3.0.1",
    "prettier-config-vuepress": "^4.4.0",
    "sort-package-json": "^2.5.1",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-order": "^6.0.3",
    "tsconfig-vuepress": "^4.2.0",
    "vite-plugin-inspect": "^0.7.35",
    "zhlint": "^0.7.1"
  },
  "packageManager": "pnpm@8.6.12"
}

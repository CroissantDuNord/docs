{
  "private": true,
  "homepage": "https://yuanshen.site/docs/",
  "license": "MIT",
  "author": " <jiazengp@gmail.com> (^_^)",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "npx git-cz",
    "docs:build": "vuepress build docs --clean-cache",
    "docs:build2": "vuepress build docs",
    "docs:clean": "rimraf docs/.vuepress/.temp docs/.vuepress/.cache ./dist",
    "docs:dev": "vuepress dev docs --clean-cache",
    "docs:info": "vuepress info",
    "lint": "yarn lint:prettier --write && yarn lint:eslint --fix",
    "lint:check": "yarn lint:prettier && yarn lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts,.vue",
    "lint:md": "yarn lint:md-lint && yarn lint:md-remark",
    "lint:md-lint": "markdownlint **/*.md",
    "lint:md-remark": "remark --quiet .",
    "lint:prettier": "prettier --check .",
    "packages:check-update": "ncu --deep --timeout 600000",
    "packages:doctor": "ncu --doctor -u",
    "packages:major": "npm taze major",
    "packages:minor": "npx taze minor",
    "packages:sort": "sort-package-json",
    "packages:taze": "npx taze -r",
    "packages:update": "ncu -u --deep --timeout 600000",
    "prepare": "husky install",
    "preview": "vite preview --port 8080 --host",
    "preview-https": "serve dist"
  },
  "lint-staged": {
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": "prettier-config-vuepress",
  "remarkConfig": {
    "plugins": [
      "frontmatter",
      [
        "footnotes",
        {
          "inlineNotes": true
        }
      ],
      "math",
      "preset-lint-consistent",
      "preset-lint-recommended",
      "preset-lint-markdown-style-guide",
      [
        "lint-maximum-line-length",
        false
      ],
      [
        "lint-list-item-spacing",
        false
      ],
      [
        "lint-list-item-indent",
        "space"
      ],
      [
        "lint-emphasis-marker",
        false
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/concurrently": "6.4.0",
    "@types/eslint": "8.4.1",
    "@types/markdown-it": "12.2.3",
    "@types/node": "17.0.18",
    "@types/nprogress": "0.2.0",
    "@types/qrcode": "1.4.2",
    "@types/qs": "6.9.7",
    "@types/semver": "7.3.9",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vuedx/typescript-plugin-vue": "0.7.4",
    "@vuepress/bundler-webpack": "next",
    "@vuepress/plugin-docsearch": "next",
    "@vuepress/plugin-git": "next",
    "@vuepress/plugin-google-analytics": "next",
    "@vuepress/plugin-register-components": "next",
    "@vuepress/plugin-search": "next",
    "@vuepress/plugin-shiki": "next",
    "@vuepress/plugin-toc": "next",
    "@vueuse/core": "7.6.2",
    "axios": "0.26.0",
    "concurrently": "7.0.0",
    "conventional-changelog-cli": "2.2.2",
    "cpx2": "4.2.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "dayjs": "1.10.7",
    "default-passive-events": "2.0.0",
    "dotenv": "16.0.0",
    "element-plus": "1.0.2-beta.70",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-vuepress": "3.5.0",
    "eslint-config-vuepress-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-prettier-vue": "3.1.0",
    "https-localhost": "4.7.0",
    "husky": "7.0.4",
    "js-md5": "0.7.3",
    "lint-md": "0.2.0",
    "lint-staged": "12.3.4",
    "markdownlint-cli": "0.31.1",
    "mocha": "9.2.0",
    "npm-check-updates": "12.3.0",
    "prettier": "2.5.1",
    "prettier-config-vuepress": "1.3.0",
    "qrcode": "1.5.0",
    "qs": "6.10.3",
    "release-it": "14.12.4",
    "screenfull": "6.0.1",
    "semantic-release": "^19.0.2",

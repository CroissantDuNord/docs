{
  "private": "true",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vitepress build",
    "build-mpa": "vitepress build --mpa",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "dev": "vitepress",
    "postinstall": "husky install",
    "lint": "prettier  --write . && eslint --fix --ext .js,.ts,.vue",
    "lint:check": "pnpm lint:prettier && pnpm lint:eslint && pnpm lint:zh",
    "lint:eslint": "eslint . --ext .js,.ts,.vue",
    "lint:md": "markdownlint **/*.md && lint:zh",
    "lint:prettier": "prettier --check .",
    "lint:zh": "zhlint 'docs/**/*.md'",
    "serve": "vitepress serve"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml}": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "prettier": "prettier-config-vuepress",
  "dependencies": {
    "@iconify-json/ic": "^1.1.13",
    "@iconify-json/logos": "^1.1.31",
    "@iconify-json/mdi": "^1.1.52",
    "@iconify-json/ph": "^1.1.5",
    "@mdit/plugin-container": "^0.4.7",
    "@mdit/plugin-mark": "^0.4.7",
    "@types/js-yaml": "^4.0.5",
    "@vueuse/core": "^9.13.0",
    "@vueuse/integrations": "^9.13.0",
    "dayjs": "^1.11.8",
    "feed": "^4.2.2",
    "js-yaml": "^4.1.0",
    "markdown-it-footnote": "^3.0.3",
    "medium-zoom": "^1.0.8",
    "qrcode": "^1.5.3",
    "sass": "^1.63.2",
    "sass-loader": "^13.3.1",
    "sitemap": "^7.1.1",
    "typescript": "^4.9.5",
    "unocss": "^0.45.30",
    "unplugin-auto-import": "^0.11.5",
    "unplugin-vue-components": "^0.22.12",
    "vitepress": "1.0.0-beta.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue-router-better-scroller": "^0.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/types": "^17.4.4",
    "@mdit/plugin-figure": "^0.4.7",
    "@mdit/plugin-img-lazyload": "^0.4.7",
    "@mdit/plugin-img-mark": "^0.4.7",
    "@mdit/plugin-img-size": "^0.4.7",
    "@mdit/plugin-sub": "^0.4.7",
    "@mdit/plugin-sup": "^0.4.7",
    "@types/eslint": "^8.40.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@unocss/eslint-config": "^0.51.13",
    "@vite-pwa/vitepress": "^0.0.5",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.42.0",
    "eslint-config-vuepress": "^3.10.0",
    "eslint-config-vuepress-typescript": "^2.9.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-vue": "9.1.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "markdown-it": "^13.0.1",
    "markdownlint-cli": "^0.31.1",
    "pnpm": "^8.6.1",
    "prettier": "^2.8.8",
    "prettier-config-vuepress": "^1.4.0",
    "sort-package-json": "^1.57.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-order": "^5.0.0",
    "tsconfig-vuepress": "^0.2.1",
    "vite-plugin-inspect": "^0.7.28",
    "zhlint": "^0.6.2"
  },
  "packageManager": "pnpm@8.6.1"
}

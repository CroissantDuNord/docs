    "sort-package-json": "1.54.0",
    "typescript": "4.5.5",
    "vuepress": "next",
    "vuepress-plugin-copy-code2": "next",
    "vuepress-plugin-feed2": "next",
    "vuepress-plugin-lightgallery": "next",
    "vuepress-plugin-md-enhance": "next",
    "vuepress-plugin-pwa2": "next",
    "vuepress-plugin-sass-palette": "next",
    "vuepress-plugin-seo2": "next",
    "vuepress-plugin-sitemap2": "next",
    "yaml": "2.0.0-7"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
